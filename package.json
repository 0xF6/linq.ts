{
  "name": "linqable.ts",
  "version": "1.7.5",
  "description": "LINQ implementation library for TypeScript",
  "main": "./build/Extensions.js",
  "author": "Yuuki Wesp",
  "license": "MIT",
  "private": false,
  "keywords": [
    "typescript",
    "linq",
    "linqjs",
    "csharp",
    "query",
    "lang",
    "linqable",
    "enumerable",
    "functional programming",
    "microsoft",
    "dotnet",
    ".net",
    "csharp-linq"
  ],
  "bugs": {
    "url": "https://github.com/0xF6/linqable.ts/issues"
  },
  "homepage": "https://github.com/0xF6/linqable.ts#readme",
  "scripts": {
    "build": "tsc",
    "test": "ava test/**/*.test.ts --verbose",
    "test-nya": "ava test/**/*.test.ts --tap | tap-nyan",
    "report-test": "yarn nyc --reporter=lcov --reporter=text-lcov ava test/**/*.test.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/0xF6/linqable.ts.git"
  },
  "devDependencies": {
    "@types/node": "^10.1.4",
    "ava": "^1.0.0-beta.6",
    "inline-source-map": "^0.6.2",
    "node-ts": "^2.1.2",
    "nyc": "^13.0.0",
    "source-map-support": "^0.5.6",
    "tap-nyan": "^1.1.0",
    "ts-loader": "^4.3.1",
    "ts-node": "^6.1.1",
    "typescript": "^2.8.4"
  },
  "nyc": {
    "extension": [
      ".ts"
    ],
    "exclude": [
      "**/*.d.ts",
      "**/*.js",
      "test"
    ],
    "reporter": [
      "html",
      "lcov",
      "text"
    ],
    "all": true,
    "cache": true
  },
  "ava": {
    "compileEnhancements": false,
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register"
    ]
  }
}